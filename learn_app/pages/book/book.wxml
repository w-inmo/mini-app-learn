<view class="container">
    <view class="header">
        <view class="box">
            <image src="images/search.png" />
            <text>搜索书籍</text>
        </view>
    </view>
    <view class="sub-container">
        <image class="head-img" src="images/quality.png" />
        <view class="books-container">
            <block wx:for="{{ books }}" wx:key="id">
                <i-book book="{{ item }}" ></i-book>
            </block>
        </view>
    </view>
</view>

